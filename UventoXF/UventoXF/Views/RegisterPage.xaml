<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UventoXF.Views.RegisterPage">
    <StackLayout>
        <Entry Placeholder="First Name" Keyboard="Text" x:Name="FirstNameEntry"/>
        <Entry Placeholder="Last Name" Keyboard="Text" x:Name="LastNameEntry"/>
        <Entry Placeholder="Email" Keyboard="Email" x:Name="EmailEntry"/>
        <Entry Placeholder="Password" IsPassword="true" x:Name="PasswordEntry"/>

        <Button Text="Register" Clicked="OnRegisterButtonClicked"/>
    </StackLayout>
    
</ContentPage>